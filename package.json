  "scripts": {
    "cm": "cz",
    "dev": "npm run watch:workers & next dev -p 3333",
    "build:workers": "NODE_ENV=production && node buildWorkers.js",
    "watch:workers": "NODE_ENV=development && node buildWorkers.js",
    "build": "npm run build:workers && next build",
    "start": "next start",
    "test": "jest --maxWorkers=50% --coverage --collectCoverageFrom='./(src|pages)/**/*.ts(x)?' --collectCoverageFrom='!./(src|pages)/**/*.stories.ts(x)?'",
    "test:watch": "npm run test -- --watch",
    "test:ci": "jest --runInBand",
    "cypress:open": "cypress open",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint-fix": "npm run lint -- --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "proxy": "http://localhost:4000",
  "msw": {
    "workerDirectory": "public"
